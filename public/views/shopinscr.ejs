    <label for="floatingAddress"><i class="fa-solid fa-envelope m-2"></i>Address</label>
  </div>
  <div class="form-floating mb-3 w-50">
    <input type="number" class="form-control" id="myNumber" style="border:2px solid blue" placeholder="Numero:">
    <label for="floatingNumero"><i class="fa-solid fa-envelope m-2"></i>Numero</label>
  </div>
  <div class="form-floating mb-3 w-50">
    <input type="email" class="form-control" id="myMail" style="border:2px solid blue" placeholder="Email:">
    <label for="floatingEmail"><i class="fa-solid fa-envelope m-2"></i>Email</label>
  </div>
  <div class="form-floating mb-3 w-50">
    <input type="password" class="form-control" id="myPass" style="border:2px solid blue" placeholder="Password:">
    <label for="floatingPassword"><i class="fa-solid fa-lock m-2"></i>Password</label>
  </div>
  <div class="col-auto">
    <button type="submit" class="btn btn-danger mt-3" id="btn">Inscription<i class="fa-solid fa-user-plus m-2"></i></button>
